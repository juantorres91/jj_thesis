def B_Init():

    from model import m
    from pyomo.environ import*
    from pyomo.opt import SolverFactory

    model=ConcreteModel()
    model.block=m

    def Obj_rule(model):
        return (model.block.y_prop[1,'CP']-60)**2

    opt=SolverFactory('bonmin')
    results=opt.solve(model, tee=True)
    return block
